class Animal:
    def init(self, name, age):
        self.name = name
        self.age = age

    def get_description(self):
        return f"Имя: {self.name}, Возраст: {self.age}"

class Zebra(Animal):
    def init(self, name, age, stripes):
        super().init(name, age)
        self.stripes = stripes

    def get_description(self):
        return f"{super().get_description()}, Вид: Зебра, Полосы: {self.stripes}"

class Dolphin(Animal):
    def init(self, name, age, fin_size):
        super().init(name, age)
        self.fin_size = fin_size

    def get_description(self):
        return f"{super().get_description()}, Вид: Дельфин, Размер плавника: {self.fin_size}"

# Создание объектов
zebra = Zebra("Зорро", 5, "Черно-белые")
dolphin = Dolphin("Флиппер", 8, "Средний")

# Вывод описаний
print(zebra.get_description())  # Вывод: Имя: Зорро, Возраст: 5, Вид: Зебра, Полосы: Черно-белые
print(dolphin.get_description())  # Вывод: Имя: Флиппер, Возраст: 8, Вид: Дельфин, Размер плавника: Средний
